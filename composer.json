{
  "name": "ricardokovalski/feature-toggle",
  "description": "Implementação de Feature Toggles com PHP.",
  "type": "library",
  "authors": [
    {
      "name": "Ricardo Kovalski Cruz",
      "email": "ricardokovalskicruz@gmail.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "keywords": ["adapter", "feature-flag", "feature-flags", "feature-toggle", "feature-toggles", "php", "php82"],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "splitsoftware/split-sdk-php": "7.1.6",
    "illuminate/support": "^10.3",
    "vlucas/phpdotenv": "^5.5",
    "symfony/var-dumper": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "RicardoKovalski\\Toggles\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
